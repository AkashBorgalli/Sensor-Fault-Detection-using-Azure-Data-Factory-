{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "akashb-df"
		},
		"AzureBlobStorage_Bad_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Bad'"
		},
		"AzureBlobStorage_Good_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Good'"
		},
		"AzureBlobStorage_Raw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_Raw'"
		},
		"general_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'general'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"ls_new_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_new'"
		},
		"ref_schema_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ref_schema_ls'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "akashb-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_Bad_connectionString": {
						"value": "[parameters('AzureBlobStorage_Bad_connectionString')]"
					},
					"AzureBlobStorage_Good_connectionString": {
						"value": "[parameters('AzureBlobStorage_Good_connectionString')]"
					},
					"AzureBlobStorage_Raw_connectionString": {
						"value": "[parameters('AzureBlobStorage_Raw_connectionString')]"
					},
					"general_connectionString": {
						"value": "[parameters('general_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"ls_new_connectionString": {
						"value": "[parameters('ls_new_connectionString')]"
					},
					"ref_schema_ls_connectionString": {
						"value": "[parameters('ref_schema_ls_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "akashb-df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'akashb-df_ArmTemplate_0')]"
			]
		}
	]
}